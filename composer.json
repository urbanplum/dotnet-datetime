{
    "name": "urbanplum/dotnet-datetime",
    "description": "Tools to convert between .NET and PHP date formats",
    "keywords": [
        "urbanplum",
        "dotnet-datetime",
        ".net",
        "php",
        "date",
        "time",
        "datetime",
        "format",
        "converter"
    ],
    "homepage": "https://github.com/urbanplum/dotnet-datetime",
    "license": "MIT",
    "authors": [
        {
            "name": "John Smith",
            "email": "hello@johnsmith.io",
            "homepage": "http://johnsmith.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "symfony/var-dumper": "~2.7"
    },
    "autoload": {
        "psr-4": {
            "Urbanplum\\DotnetDateTime\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Urbanplum\\DotnetDateTime\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit ./tests"
    }
}
